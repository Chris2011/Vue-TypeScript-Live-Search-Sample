{
    "name": "vue-typescript-live-search-sample",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "npm run svg2png && vue-cli-service serve",
        "build": "npm run test && npm run svg2png && vue-cli-service build",
        "test:unit": "vue-cli-service test:unit",
        "test:e2e": "vue-cli-service test:e2e",
        "lint": "vue-cli-service lint",
        "analyze-package": "webpack-bundle-analyzer ./reports/webpack-bundle-analyzer/stats.json",
        "sourcemap-explorer": "source-map-explorer ./dist/app.js ./dist/app.js.map",
        "svg2png": "svg2png-many -i ./src/icons -o ./.tmp/icons -w 20 -h 20 && node ./build-scripts/gen-sprite.js",
        "ver": "webpack -v"
    },
    "dependencies": {
        "core-js": "^3.4.3",
        "vue": "^2.6.10",
        "vue-class-component": "^7.0.2",
        "vue-property-decorator": "^8.3.0",
        "vuex": "^3.1.0"
    },
    "devDependencies": {
        "@types/jest": "^24.0.19",
        "@vue/cli-plugin-babel": "^4.1.0",
        "@vue/cli-plugin-e2e-cypress": "^4.1.0",
        "@vue/cli-plugin-typescript": "^4.1.0",
        "@vue/cli-plugin-unit-jest": "^4.1.0",
        "@vue/cli-service": "^4.1.0",
        "@vue/test-utils": "1.0.0-beta.29",
        "node-sass": "^4.12.0",
        "node-sprite-generator": "^0.10.2",
        "sass-loader": "^8.0.0",
        "source-map-explorer": "^1.8.0",
        "svg-sprite": "^1.4.0",
        "svg-sprite-loader": "^3.9.0",
        "svg2png-many": "git+https://github.com/Chris2011/svg2png-many.git#feature/2-fix-missing-dir",
        "typescript": "~3.5.3",
        "vue-template-compiler": "^2.6.10",
        "vuex-module-decorators": "^0.9.8",
        "webpack-bundle-analyzer": "^2.13.1"
    }
}