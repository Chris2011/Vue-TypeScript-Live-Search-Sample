{
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "endOfLine": "lf",
    "arrowParens": "always",
    "jsxBracketSameLine": true,
    "htmlWhitespaceSensitivity": "ignore",
    "printWidth": 150,
    "overrides": [{
        "files": "*.vue",
        "options": {
            "parser": "vue"
        }
    }]
}